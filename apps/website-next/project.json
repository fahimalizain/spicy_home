{
  "name": "website-next",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/website-next",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/website-next"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/website-next"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "website-next:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "website-next:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "website-next:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "website-next:build:production"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/website-next/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": []
}
